{
  "name": "react-webui",
  "version": "0.1.0",
  "private": true,
  "package-lock": false,
  "dependencies": {
    "@sparkpost/matchbox": "^1.7.9",
    "axios": "^0.16.1",
    "babel-plugin-lodash": "^3.2.11",
    "chart.js": "^2.6.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "date-fns": "^1.28.5",
    "downshift": "^1.11.0",
    "history": "^4.6.1",
    "jest-in-case": "^1.0.2",
    "js-cookie": "^2.1.4",
    "moment": "2.18.1",
    "payment": "^2.3.0",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-chartjs-2": "^2.1.0",
    "react-day-picker": "^6.1.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "recharts": "^1.0.0-alpha.2",
    "redux": "^3.6.0",
    "redux-form": "7.0.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "source-map-explorer": "^1.5.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-recharts": "^1.1.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.23.0",
    "brace": "^0.10.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "cssnano": "^3.10.0",
    "dotenv": "4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.2",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-sparkpost": "^1.3.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "^21.1.0",
    "jest-enzyme": "^4.0.0",
    "jest-junit": "^2.1.0",
    "node-sass": "^4.5.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-ace": "^5.1.1",
    "react-dev-utils": "^2.0.1",
    "react-error-overlay": "^1.0.6",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.5",
    "style-ext-html-webpack-plugin": "^3.4.1",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src",
    "autolint": "eslint src -- --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "modulePaths": [
      ""
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "phantomjs": true
    },
    "rules": {
      "block-spacing": [
        "error",
        "always"
      ],
      "complexity": "off",
      "react/jsx-no-bind": "off",
      "max-lines": [
        "error",
        {
          "max": 150,
          "skipComments": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": false,
          "objectsInObjects": false
        }
      ],
      "one-var": [
        "error",
        "never"
      ],
      "operator-linebreak": [
        "error",
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before"
          }
        }
      ]
    },
    "extends": [
      "react-app",
      "sparkpost",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "jest"
    ]
  },
  "description": "SparkPost web app built in React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparkPost/2web2ui.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/SparkPost/2web2ui/issues"
  }
}
