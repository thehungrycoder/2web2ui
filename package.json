    "email-addresses": "^3.0.1",
    "history": "^4.6.1",
    "jest-in-case": "^1.0.2",
    "js-cookie": "^2.1.4",
    "localforage": "^1.5.3",
    "moment": "2.20.1",
    "papaparse": "^4.3.6",
    "payment": "^2.3.0",
    "query-string": "^5.0.0",
    "raven-for-redux": "^1.1.1",
    "raven-js": "^3.19.1",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.1.0",
    "react-day-picker": "^7.0.7",
