{
  "name": "react-webui",
  "version": "0.1.0",
  "private": true,
  "package-lock": true,
  "engines": {
    "npm": ">=5.3"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=local-$USER-$(git rev-parse HEAD) node scripts/start.js",
    "build": "node scripts/build.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "node scripts/test.js --env=jsdom --config=./jest.config.js",
    "test-coverage": "npm test -- --coverage",
    "test-ci": "CI=true npm test -- --coverage",
    "coveralls": "cat ./build-reports/lcov.info | coveralls",
    "test-examples": "node scripts/test.js --env=jsdom --config=./jest.config.js --testMatch='<rootDir>/docs/testing/**/?(*.)test.js'",
    "lint": "node node_modules/eslint/bin/eslint.js src",
    "lintfix": "npm run lint -- --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "stage-2"
    ],
    "plugins": [
      "lodash",
      "recharts"
    ]
  },
  "description": "SparkPost web app built in React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparkPost/2web2ui.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/SparkPost/2web2ui/issues"
  },
  "dependencies": {
